package com.dacheng.mes.sfdcs.pojo;

public class Assembly {

	private int assemblyKey;
	
	private String sfdsId;
	
	private String description;
	
	private String partNumber;
	
	private int steps;
	
	private String revision;

	public int getAssemblyKey() {
		return assemblyKey;
	}

	public void setAssemblyKey(int assemblyKey) {
		this.assemblyKey = assemblyKey;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getPartNumber() {
		return partNumber;
	}

	public void setPartNumber(String partNumber) {
		this.partNumber = partNumber;
	}

	public String getSfdsId() {
		return sfdsId;
	}

	public void setSfdsId(String sfdsId) {
		this.sfdsId = sfdsId;
	}



	public String getRevision() {
		return revision;
	}

	public void setRevision(String revision) {
		this.revision = revision;
	}

	public int getSteps() {
		return steps;
	}

	public void setSteps(int steps) {
		this.steps = steps;
	}

}
